key=\"$route.fullPath\" to=\"/Home\">\r\n            <div class=\"a-menu-item\">\r\n              <a-icon type=\"home\" />\r\n              <span>首页</span>\r\n            </div>\r\n          </router-link>\r\n        </a-menu-item>\r\n        <a-menu-item class=\"menu-item\" key=\"2\">\r\n          <router-link :key=\"$route.fullPath\" to=\"/Album\">\r\n            <div class=\"a-menu-item\">\r\n              <a-icon type=\"picture\" />\r\n              <span>相册集</span>\r\n            </div>\r\n          </router-link>\r\n        </a-menu-item>\r\n        <a-menu-item class=\"menu-item\" key=\"3\">\r\n          <router-link :key=\"$route.fullPath\" to=\"/Recycle\">\r\n            <div class=\"a-menu-item\">\r\n              <a-icon type=\"rest\" />\r\n              <span>回收站</span>\r\n            </div>\r\n          </router-link>\r\n        </a-menu-item>\r\n        <a-menu-item class=\"menu-item\" key=\"4\">\r\n          <router-link :key=\"$route.fullPath\" to=\"/Record\">\r\n            <div class=\"a-menu-item\">\r\n              <a-icon type=\"carry-out\" />\r\n              <span>操作记录</span>\r\n            </div>\r\n          </router-link>\r\n        </a-menu-item>\r\n\r\n        <a-sub-menu class=\"menu-item\" key=\"5\">\r\n          <span slot=\"title\">\r\n            <div class=\"a-menu-item\">\r\n              <a-icon type=\"area-chart\" />\r\n              <span>数据可视</span>\r\n            </div>\r\n          </span>\r\n        <a-menu-item class=\"menu-item\" key=\"6\">\r\n          <router-link :key=\"$route.fullPath\" to=\"/VisualdataType\">\r\n            <div class=\"a-menu-item\">\r\n              <span>图片类型</span>\r\n            </div>\r\n          </router-link>\r\n        </a-menu-item>\r\n        <a-menu-item class=\"menu-item\" key=\"7\">\r\n          <router-link :key=\"$route.fullPath\" to=\"/VisualdataSite\">\r\n            <div class=\"a-menu-item\">\r\n              <span>图片位置</span>\r\n            </div>\r\n          </router-link>\r\n        </a-menu-item>\r\n\r\n      </a-sub-menu>\r\n        <a-sub-menu class=\"menu-item\" key=\"8\">\r\n          <span slot=\"title\">\r\n            <div class=\"a-menu-item\">\r\n              <a-icon type=\"upload\" />\r\n              <span>上传</span>\r\n            </div>\r\n          </span>\r\n          <a-menu-item key=\"9\" class=\"menu-item\">\r\n            <router-link :key=\"$route.fullPath\" to=\"/upload\">\r\n              <div class=\"a-menu-item\">\r\n                <span>普通上传</span>\r\n              </div>\r\n            </router-link>\r\n          </a-menu-item>\r\n          <a-menu-item key=\"10\">\r\n            <router-link :key=\"$route.fullPath\" to=\"/AIupload\">\r\n              <div class=\"a-menu-item\">\r\n                <span>AI分类上传</span>\r\n              </div>\r\n            </router-link>\r\n          </a-menu-item>\r\n        </a-sub-menu>\r\n        <a-sub-menu class=\"menu-item\" key=\"11\">\r\n          <span slot=\"title\">\r\n            <div class=\"a-menu-item\">\r\n              <a-icon type=\"team\" />\r\n              <span>个人页面</span>\r\n            </div>\r\n          </span>\r\n          <a-menu-item key=\"12\" class=\"menu-item\">\r\n            <router-link :key=\"$route.fullPath\" to=\"/UserMid\">\r\n              <div class=\"a-menu-item\">\r\n                <span>个人中心</span>\r\n              </div>\r\n            </router-link>\r\n          </a-menu-item>\r\n          <a-menu-item key=\"13\">\r\n            <router-link :key=\"$route.fullPath\" to=\"/UserSet\">\r\n              <div class=\"a-menu-item\">\r\n                <span>个人设置</span>\r\n              </div>\r\n            </router-link>\r\n          </a-menu-item>\r\n\r\n        </a-sub-menu>\r\n      </a-menu>\r\n    </a-layout-sider>\r\n    <a-layout>\r\n      <a-layout-header style=\"background: #fff; text-align:left;padding: 0 10px;\">\r\n        <a-icon class=\"trigger h-a-icon\" :type=\"collapsed ? 'menu-unfold' : 'menu-fold'\"\r\n          @click=\"() => (collapsed = !collapsed)\" />\r\n        <a-icon class=\"h-a-icon\" type=\"reload\" @click=\"refresh\"> </a-icon>\r\n\r\n        <a-dropdown style=\"float:right ; margin-right:25px\">\r\n          <a class=\"ant-dropdown-link\" @click=\"e => e.preventDefault()\">\r\n            <a-avatar :size=\"36\" :src=\"serveUrL+user.avatar\" /><span style=\"margin:18px ;font-size:18px ;margin-top:5px\">{{\r\n                user.userName\r\n            }}</span>\r\n          </a>\r\n          <a-menu slot=\"overlay\">\r\n            <a-menu-item key=\"14\">\r\n              <router-link :key=\"$route.fullPath\" to=\"/UserMid\">\r\n                <a target=\"_blank\" rel=\"noopener noreferrer\">个人中心</a>\r\n              </router-link>\r\n            </a-menu-item>\r\n            <a-menu-item key=\"15\">\r\n              <router-link :key=\"$route.fullPath\" to=\"/UserSet\">\r\n                <a target=\"_blank\" rel=\"noopener noreferrer\">个人设置</a>\r\n              </router-link>\r\n            </a-menu-item>\r\n            <a-menu-divider />\r\n\r\n            <a-menu-item key=\"16\">\r\n              <router-link :key=\"$route.fullPath\" to=\"/\">\r\n                退出登录\r\n              </router-link>\r\n            </a-menu-item>\r\n          </a-menu>\r\n        </a-dropdown>\r\n      </a-layout-header>\r\n      <a-layout-content id=\"a-layout-content\">\r\n<!-- 向子组件传入user对象 -->\r\n        <router-view :userprop=\"user\" />\r\n      \r\n      </a-layout-content>\r\n      <a-layout-footer style=\"text-align: center\">\r\n        F-C-A\r\n      </a-layout-footer>\r\n\r\n    </a-layout>\r\n  </a-layout>\r\n</template>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      user: {\r\n        avatar: \"/static/avatar/default.jpg\",\r\n        birthday: \"2022-08-20 00:00:00\",\r\n        capacity: 1000,\r\n        city: \"陕西,西安\",\r\n        email: \"1609048001@qq.com\",\r\n        passWord: \"123456\",\r\n        phone: \"13891531948\",\r\n        userId: 1,\r\n        userName: \"xiaohua\",\r\n    },\r\n      token: '11', //token凭证\r\n      serveUrL:this.$serveUrL,\r\n      collapsed: false,\r\n      index: '1',\r\n     \r\n     \r\n    };\r\n  },\r\n  created() {\r\n    this.RecoverFocus();\r\n  },\r\n  mounted() {\r\n    var cookie = this.$cookie.getCookie();\r\n    if (cookie[0] != \"token\") {\r\n      this.$message({\r\n        message: '登录已过期!',\r\n        type: 'error'\r\n      });\r\n      this.$router.push({ name: 'login' });\r\n    }\r\n    else {\r\n      this.token = cookie[1];\r\n    }\r\n    this.selectUser();\r\n  \r\n \r\n  },\r\n  methods: {\r\n    RecoverFocus() {\r\n      // 使刷新后蓝色焦点选中路由对应的item\r\n      var paths = ['/Home', '/Album', '/Recycle', '/Record','','/VisualdataType','/VisualdataSite', '', '/upload', '/AIUpload', '', '/UserMid', '/UserSet'];\r\n      var path = this.$route.path;\r\n      var i = 0;\r\n      for (; i < paths.length; i++) {\r\n        if (path == (paths[i])) {\r\n          break;\r\n        }\r\n      }\r\n      this.index = (i + 1) + \"\";\r\n      console.log(path)\r\n    },\r\n    refresh() {\r\n      this.$router.go(0);\r\n    },\r\n    selectUser() {\r\n      var _this = this;\r\n       let data = new FormData();\r\n       data.append(\"token\",this.token);\r\n      this.axios({\r\n        url: this.$serveUrL + \"/user/selectUser\",\r\n        method: \"post\",\r\n        data: data,\r\n        headers: {\r\n          'Content-Type': 'application/x-www-form-urlencoded'\r\n        }\r\n      }).then(function (resp) {\r\n        var data = resp.data;\r\n        if (data.status == \"success\") {\r\n         _this.user = data.user;\r\n        }\r\n      }\r\n      )\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style>\r\n#components-layout-demo-side .logo {\r\n  height: 32px;\r\n  margin: 16px;\r\n  overflow: hidden;\r\n}\r\n\r\n.logo_txt {\r\n  display: inline-block;\r\n  color: #fff;\r\n  font-size: 20px;\r\n  font-family: Avenir, Helvetica Neue, Arial, Helvetica, sans-serif;\r\n  font-weight: 800;\r\n  margin: 0;\r\n  padding: 0;\r\n  margin-left: 10px;\r\n  margin-bottom: 28px;\r\n  vertical-align: middle;\r\n}\r\n\r\n/* logo */\r\n.logo_img {\r\n  display: inline-block;\r\n  width: 32px !important;\r\n  height: 36px !important;\r\n  /* margin-left: 10px;\r\n  margin-bottom: 28px; */\r\n}\r\n\r\n#a-menu {\r\n  text-align: left;\r\n\r\n}\r\n\r\n.menu-item {\r\n  margin-top: 20px !important;\r\n}\r\n\r\n.a-menu-item {\r\n  margin-left: 20%;\r\n}\r\n\r\n/* 折叠与刷新图标 */\r\n.h-a-icon {\r\n  font-size: 18px;\r\n  margin-left: 18px;\r\n}\r\n</style>\r\n"]}]}